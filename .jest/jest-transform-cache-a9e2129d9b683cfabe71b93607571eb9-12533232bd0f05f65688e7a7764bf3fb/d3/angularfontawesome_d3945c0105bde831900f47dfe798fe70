function ensureCss(document, config2) {
  if (!config2.autoAddCss) {
    return;
  }
  if (cssInserted.has(document)) {
    return;
  }
  if (document.getElementById(autoCssId) != null) {
    config2.autoAddCss = false;
    cssInserted.add(document);
    return;
  }
  const style = document.createElement("style");
  style.setAttribute("type", "text/css");
  style.setAttribute("id", autoCssId);
  style.innerHTML = import_fontawesome_svg_core.dom.css();
  const headChildren = document.head.childNodes;
  let beforeChild = null;
  for (let i = headChildren.length - 1; i > -1; i--) {
    const child = headChildren[i];
    const tagName = child.nodeName.toUpperCase();
    if (["STYLE", "LINK"].indexOf(tagName) > -1) {
      beforeChild = child;
    }
  }
  document.head.insertBefore(style, beforeChild);
  config2.autoAddCss = false;
  cssInserted.add(document);
}
